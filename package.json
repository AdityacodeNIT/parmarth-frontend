{
  "name": "adyog",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "This project is set up with Vite.",
  "main": "tailwind.config.js",
  "keywords": [
    "react",
    "vite",
    "ecommerce",
    "frontend",
    "tailwindcss"
  ],
  "author": "Aditya",
  "license": "ISC",
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@google/generative-ai": "^0.24.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-three/drei": "^10.0.2",
    "@react-three/fiber": "^9.0.4",
    "@reduxjs/toolkit": "^2.8.2",
    "@vercel/speed-insights": "^1.1.0",
    "axios": "^1.6.8",
    "babel-plugin-glsl": "^1.0.0",
    "bootstrap": "^5.3.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "express": "^4.21.2",
    "framer-motion": "^12.23.24",
    "gsap": "^3.12.7",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.525.0",
    "process": "^0.11.10",
    "razorpay": "^2.9.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.3.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.27.0",
    "react-slick": "^0.30.3",
    "redux-persist": "^6.0.0",
    "slick-carousel": "^1.8.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.173.0",
    "vite": "^5.4.9",
    "vite-plugin-svgr": "^4.2.0",
    "web-vitals": "^4.2.3",
    "yup": "^1.7.1"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "build:profile": "vite build --mode profile",
    "serve": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,css,md}",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:performance": "vitest run --reporter=verbose src/**/*.perf.test.js",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "bundle-size": "npx bundlesize",
    "preload-routes": "node scripts/preload-routes.js",
    "optimize-images": "node scripts/optimize-images.js",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/ui": "^1.6.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^27.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^4.1.17",
    "vitest": "^1.6.1"
  }
}
